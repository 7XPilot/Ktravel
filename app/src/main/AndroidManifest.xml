<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.k_travel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ktravel">
        <activity android:name=".UserInfoActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".SendEmailActivity" />
        <activity android:name=".ScenceDetailActivity" />
        <activity android:name=".base.RegisterSetPwdActivity" />
        <activity android:name=".base.LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".base.ResetPwdActivity" />
        <activity android:name=".base.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>